[alias]
	compare = log --oneline master..
	author = shortlog -s -e
	ci = commit
	co = checkout
	br = branch
	st = status -s
	edit = "!vim `git ls-files -m` -p"
	flux = log --author="flux" --oneline
	ticket = "!open https://stackpulse.atlassian.net/browse/$(git branch --show-current)"
[push]
	default = simple
[color]
	ui = true
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[merge]
	tool = vimdiff
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[mergetool]
	path = nvim
[url "git@github.com:stackpulse"]
	insteadOf = https://github.com/stackpulse
[mergetool "vimdiff"]
	path = nvim
